{
  "name": "otc-vite-sdk-sandbox",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "sdk-file": "openapi --input openapi.json --output sdk_node --client axios --useOptions --useUnionTypes",
    "sdk-url": "openapi --input http://localhost:8000/openapi.json --output sdk_node --client axios --useOptions --useUnionTypes"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "form-data": "^4.0.0",
    "openapi-typescript-codegen": "^0.25.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
